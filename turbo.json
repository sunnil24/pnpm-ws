{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**"]
      },
      "lint": {
        "dependsOn": ["^build"]
      },
      "clean": {},
      "cleanmodules":{},
      "test": {
        "dependsOn": ["build"],
        "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
      },
      "dev": {
        "cache": false,
        "persistent": true
      }
    }
  }