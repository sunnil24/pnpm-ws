{
    "name": "@xyzproject/env-hook",
    "version": "1.0.4",
    "type": "module",
    "devDependencies": {
      "tsup": "7.2.0",
      "copyfiles": "2.4.1"
    },
    "types": "./dist/index.d.ts",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "publishConfig": {
