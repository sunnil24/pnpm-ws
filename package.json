{
  "name": "sldl",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@chromatic-com/storybook": "^3",
    "@commitlint/config-conventional": "^18.4.4",
    "@commitlint/cz-commitlint": "^18.4.4",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@jsdevtools/version-bump-prompt": "6.1.0",
    "@repo/typescript-config": "workspace:*",
    "@sldl/commit-lint-standards": "workspace:*",
    "@sldl/react-linting-standards": "workspace:*",
    "@sldl/tsup-config": "workspace:*",
    "@storybook/addon-a11y": "^8.4.7",
    "@storybook/addon-docs": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-themes": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/builder-vite": "^8.4.7",
    "@storybook/manager-api": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@storybook/theming": "^8.4.7",
    "@testing-library/jest-dom": "6.2.0",
    "@testing-library/react": "14.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "^1.2.1",
    "autoprefixer": "10.4.16",
    "class-variance-authority": "^0.7.0",
    "commitizen": "^4.3.0",
    "commitlint": "^18.4.4",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-tailwindcss": "^3.14.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.9",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react": "^19.0.0",
    "react-docgen-typescript": "2.2.2",
    "react-dom": "^19.0.0",
    "remark-gfm": "4.0.0",
    "rimraf": "5.0.5",
    "storybook": "^8.4.7",
    "tailwind-config-viewer": "1.7.3",
    "tailwindcss": "^3.4.1",
    "tsup": "7.2.0",
    "turbo": "latest",
    "typescript": "5.3.3",
    "validate-branch-name": "^1.3.0",
    "vite": "4.5.1",
    "vite-plugin-dts": "3.7.0",
    "vitest": "^1.2.1"
  },
  "scripts": {
    "clean": "turbo run clean",
    "cleanmodules": "turbo run cleanmodules && rm -rf ./node_modules ./pnpm-lock.yaml",
    "build": "turbo run build",
    "dev": "turbo run dev",
    "build:package": "node scripts/build-and-publish-package.js",
    "publish:package": "node scripts/build-and-publish-package.js",
    "build:storybook": "storybook build -o dist/storybook",
    "build:plop": "pnpm --filter @sldl/generate-plop build",
    "publish:plop": "pnpm --filter @sldl/generate-plop publish",
    "tcv": "npx tailwind-config-viewer -o -c tailwind.config.js",
    "build:tcv": "rimraf dist/tcv && mkdir -p dist/tcv && npx tailwind-config-viewer -c tailwind.config.viewer.js export dist/tcv",
    "test": "vitest --config ./vitest.config.ts",
    "test:watch": "vitest --watch --config ./vitest.config.ts",
    "coverage": "vitest run --coverage --config ./vitest.config.ts",
    "storybook": "storybook dev -p 6006",
    "generate": "generate",
    "lint": "eslint .",
    "lint:fix": "eslint '**/*.{ts,tsx}' --fix",
    "prepare": "husky install && copyfiles -f node_modules/@sldl/commit-lint-standards/dist/.husky/* .husky",
    "commit": "validate-branch-name && cz ",
    "prepare-commit": "cross-env CHECKS_RUN=1 yarn run commit"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^2.0.3",
    "@sldl/generate-plop": "workspace:*",
    "@sldl/tailwind-config": "workspace:*",
    "ag-grid-community": "^31.0.3",
    "ag-grid-react": "^31.0.3",
    "eslint-import-resolver-typescript": "^3.6.1",
    "npm-run-all": "^4.1.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "validate-branch-name": {
    "pattern": "^(main|(feat|fix|task)/NSMA-\\d+/[\\w-]+)$",
    "errorMsg": "Invalid branch name. Examples: [main, feat/NSMA-1200/user-authentication, fix/NSMA-1234/error-handling, task/NSMA-1300/code-cleanup]"
  },
  "lint-staged": {
    "*/**/*.{ts,tsx}": [
      "eslint --fix --"
    ]
  },
  "packageManager": "pnpm@9.14.4"
}
